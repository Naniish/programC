//void my_putchar(char c);

void my_print_alpha(void){
  char letter = 'a';
   for(letter='a'; letter <= 'z'; letter++){
     printf("%c", letter); "\n";
   }
}
int main(int argc, char **argv){
    my_print_alpha();

    return (0);
}
